{
	"bundles": {
		"dist/app-build": {
			"includes": [
				"[**/*.js]",
				"**/*.html!text",
				"**/*.css!text"
			],
			"options": {
				"inject": true,
				"minify": true,
				"rev": true
			}
		},
		"dist/ext-libs": {
			"includes": [
				"rstacruz/nprogress",
				"ms-signalr-client",
				"underscore"
			],
			"options": {
				"inject": true,
				"minify": true,
				"rev": true
			}
		},
		"dist/aurelia": {
			"includes": [
				"text",
				"aurelia-bootstrapper",
				"aurelia-dependency-injection",				
				"aurelia-framework",				
				"aurelia-router",
				"aurelia-templating-binding",
				"aurelia-templating-resources",				
				"aurelia-templating-router",
				"aurelia-loader-default",
				"aurelia-http-client",
				"aurelia-history",
				"aurelia-history-browser",
				"aurelia-logging-console",
				"aurelia-event-aggregator",
				"aurelia-loader",
				"aurelia-loader-default"
			],
			"options": {
				"inject": true,
				"minify": true,
				"rev": true
			}
		}
	}
}